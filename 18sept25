

#include <iostream>
using namespace std;
int main()
{
    int n, x, s = 0, d, i, cif, cx, nr, sd;
    cin >> n;
    for(i = 1; i <= n; i++){
        cin >> x;
        cx = x;
        nr = 0;
        while(cx) {
            cif = cx% 10;
            nr = nr*10 + cif;
            cx /= 10;
        }
        
        if(nr == x){
            sd = 1; 
            
            for (d = 2; d<= nr/2; d++){
                if ((d % 2 == 1) && (nr % d == 0))
                    sd++;
            }
            if(nr%2) sd++;
            
            if(sd % 2 == 0){
                    s += nr;
            }
        }
    }
    cout << s;
    return 0;
}
